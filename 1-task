#БЫКИ И КОРОВЫ
import random
numbers = []
ans = ''
while len(numbers) != 4:
    x = random.randint(1,9)
    if x not in numbers:
        ans = ans + str(x)
        numbers.append(x)
req = '1'
buffalo = 0
cow = 0
while req != ans:
    req = input()
    for num in range(4):
        if req[num] == ans[num]:
            buffalo += 1
        elif req[num] in ans:
            cow += 1
    print(f'{buffalo} buffalo {cow} cow')
    buffalo = 0
    cow = 0
print('YOU WON')
